@model CarListViewModel

<div>
    <h2>All Cars Here</h2>
</div>

<h4 style="color:darkblue;">@Model.CurrentCategory</h4>

<br />
@{
    foreach(var car in Model.Cars)
    {
        <div>
        <h4>Car Name: @car.Name</h4>
        <p>Prioce: @car.Price.ToString("c")</p>
        </div>
    }
}









